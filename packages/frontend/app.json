{
  "expo": {
    "name": "Inro",
    "slug": "frontend",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#55B0F8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amedama.inro",
      "infoPlist": {
        "NFCReaderUsageDescription": "NFC for reading ids",
        "com.apple.developer.nfc.readersession.iso7816.select-identifiers": [
          "D392F000260100000001",
          "D3921000310001010408",
          "D3921000310001010100",
          "D3921000310001010401",
          "A0000002310100000000000000000000",
          "A0000002310200000000000000000000",
          "A0000002480300000000000000000000"
        ],
        "com.apple.developer.nfc.readersession.felica.systemcodes": [
          "0003",
          "FE00"
        ]
      },
      "entitlements": {
        "com.apple.developer.nfc.readersession.formats": [
          "TAG"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NFC"
      ],
      "package": "com.amedama.inro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5c3e1429-295c-4abd-926f-373168f91898"
      }
    }
  }
}
